{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "vuejs: chrome",
      "url": "http://localhost:8080/tr-logic-test/",
      "webRoot": "${workspaceFolder}/src",
      "sourceMapPathOverrides": {
        "webpack:///App.vue": "${webRoot}/App.vue",
        "webpack:///./router.js": "${webRoot}/router.js",
        "webpack:///views/*": "${webRoot}/views/*",
        "webpack:///./plugins/*": "${webRoot}/plugins/*",
        "webpack:///./store/*": "${webRoot}/store/*",
        "webpack:///./components/*": "${webRoot}/components/*",
      },
    }
  ]
}
